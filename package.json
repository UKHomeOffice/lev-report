{
  "name": "lev-report",
  "version": "0.4.3",
  "description": "Reports showing usage of LEV.",
  "main": "src/server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "nodemon -w ./assets/js -x 'npm run test:ui && npm run browserify' & MOCK=true nodemon -w ./src -w ./public -w ./node_modules/lev-react-components/dist/index.js ./src/server.js",
    "test": "npm run test:ui && npm run test:server",
    "test:server": "eslint src && mocha test/server",
    "test:ui": "eslint assets/js",
    "watch:server": "nodemon -w src -w test/server -x 'npm run test:server'",
    "watch:ui": "nodemon -w src -w test -x 'npm run test:server'",
    "browserify": "if [ -e ./assets/js/app.js ]; then browserify ./assets/js/app.js > ./public/js/bundle.js; fi",
    "postinstall": "mkdir -p public/js public/css && npm run browserify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/lev-report.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/lev-report/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/lev-report#readme",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "bluebird": "^3.5.4",
    "browserify": "^16.2.3",
    "lev-react-components": "^0.5.3",
    "lev-react-renderer": "^1.1.0",
    "lev-restify": "^1.0.0",
    "moment": "^2.24.0",
    "pg-monitor": "^1.1.0",
    "pg-promise": "^8.6.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.6.2",
    "bunyan": "^1.8.12",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-one-variable-per-var": "0.0.3",
    "jsdom": "^15.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "mock-require": "^3.0.3",
    "nodemon": "^1.19.0",
    "proxyquire": "^2.1.0",
    "rewire": "^4.0.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  }
}
